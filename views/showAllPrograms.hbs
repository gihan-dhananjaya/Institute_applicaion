<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Table</title>
</head>
<body>
<div class="container mb-4">
  <ul class="nav nav-pills">
      <li class="nav-item">
          <a class="nav-link me-3" aria-current="page" href="/program/show-all-programs">All Progarams</a>
      </li>
        <li class="nav-item">
          <a class="nav-link active" aria-current="page" href="/program/add-program-page">Add Program</a>
      </li>
  </ul>
</div>
<hr>
<div class="container mt-3">
  <div class="table-responsive">
    <table class="table text-center table-bordered">
      <thead>
        <tr>
          <th>Program ID</th>
          <th>Program Name</th>
          <th>Duration(Month)</th>
          <th>Program Fee</th>
          <th>Option</th>
        </tr>
      </thead>
      <tbody>
        {{#each result}}
        <tr>
          <td>{{this.pro_id}}</td>
          <td>{{this.pro_name}}</td>
          <td>{{this.pro_duration}}</td>
          <td>{{this.pro_fee}}</td>
          <td>
            <a href="/program/show-program-data-page/{{this.pro_id}}" class="btn btn-primary">Details</a>
            <a href="/program/delete-program-page/{{this.pro_id}}" class="btn btn-danger">Delete</a>
            <a href="/program/update-program-page/{{this.pro_id}}" class="btn btn-warning">Update</a>
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
</body>
</html>
